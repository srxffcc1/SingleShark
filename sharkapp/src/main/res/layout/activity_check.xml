<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#6095FF"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/eyeopen"
                    android:scaleType="fitCenter" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="14"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:text="计划"></TextView>

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/swopen" />
            </LinearLayout>
            <FrameLayout

                android:id="@+id/menujh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <FrameLayout

                android:id="@+id/contentjh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#6095FF"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/eyeopen"
                    android:scaleType="fitCenter" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="14"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:text="检查方案"></TextView>

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/swopen" />
            </LinearLayout>
            <FrameLayout

                android:id="@+id/menufa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <FrameLayout

                android:id="@+id/contentfa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#6095FF"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/eyeopen"
                    android:scaleType="fitCenter" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="14"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:text="现场检查"></TextView>

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/swopen" />
            </LinearLayout>
            <FrameLayout

                android:id="@+id/menujc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <FrameLayout

                android:id="@+id/contentjc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <com.businessframehelp.widget.ViewPagerForScrollView
                            android:id="@+id/vp_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"></com.businessframehelp.widget.ViewPagerForScrollView>

                        <com.flyco.tablayout.SegmentTabLayout
                            android:id="@+id/tl_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="5dp"
                            android:paddingLeft="2dp"
                            android:paddingRight="2dp"
                            android:paddingTop="5dp"
                            tl:tl_bar_color="#ffffff"
                            tl:tl_indicator_anim_enable="true"
                            tl:tl_indicator_color="#F6CE59"
                            tl:tl_indicator_corner_radius="8dp"
                            tl:tl_textBold="SELECT" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="10dp"></LinearLayout>

                        <LinearLayout
                            android:id="@+id/contentpass"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#6095FF"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/eyeopen"
                    android:scaleType="fitCenter" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="14"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:text="现场处理措施，责令整改"></TextView>

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/swopen" />
            </LinearLayout>
            <FrameLayout

                android:id="@+id/menucl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <FrameLayout

                android:id="@+id/contentcl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#6095FF"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/eyeopen"
                    android:scaleType="fitCenter" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="14"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:text="整改复查"></TextView>

                <com.businessframehelp.widget.AutoCheckBox
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/swopen" />
            </LinearLayout>
            <FrameLayout

                android:id="@+id/menufc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <FrameLayout

                android:id="@+id/contentfc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></LinearLayout>
        </LinearLayout>
    </ScrollView>


</LinearLayout>