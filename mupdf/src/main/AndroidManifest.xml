<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="11102" android:versionName="1.11" package="com.artifex.mupdf.mini" platformBuildVersionCode="21" platformBuildVersionName="5.0.1-1624448">
    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application >
        <activity android:label="@string/app_name" android:name=".DocumentActivity" android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="file" />
                <data android:mimeType="application/pdf" />
                <data android:mimeType="application/vnd.ms-xpsdocument" />
                <data android:mimeType="application/oxps" />
                <data android:mimeType="application/x-cbz" />
                <data android:mimeType="application/epub+zip" />
                <data android:mimeType="text/xml" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="file" />
                <data android:pathPattern=".*\.pdf" />
                <data android:pathPattern=".*\.xps" />
                <data android:pathPattern=".*\.oxps" />
                <data android:pathPattern=".*\.cbz" />
                <data android:pathPattern=".*\.epub" />
                <data android:pathPattern=".*\.fb2" />
            </intent-filter>
        </activity>
        <activity android:name=".OutlineActivity" android:configChanges="keyboardHidden|orientation|screenSize" />
    </application>
</manifest>
